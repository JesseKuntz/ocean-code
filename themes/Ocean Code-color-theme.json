        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#00A2FF"
      }
    },
    {
      "name": "HTML Tag",
      "scope": [
        "entity.name.tag.html",
      ],
      "settings": {
        "foreground": "#50BAA9"
